@article{laurent_1961_effect,
  title = {The Effect of Hyaluronic Acid on the Sedimentation Rate of Other Substances},
  author = {Laurent, Torvard C. and Pietruszkiewicz, Adolph},
  year = {1961},
  month = may,
  volume = {49},
  pages = {258--264},
  issn = {0006-3002},
  doi = {10.1016/0006-3002(61)90125-1},
  abstract = {An investigation of the sedimentation of various substances with molecular diameters of 100\textendash 3650 \AA{} in hyaluronic acid media has revealed the existence of an exponential relationship between the decrease in the sedimentation rate of a given particle and the square root of the hyaluronic acid concentration. Decreases in sedimentation rates were found to be complex functions of the molecular dimensions of the sedimenting particles but did not appear to depend on the concentration of electrolytes or on the pH. Hyaluronic acid samples which varied about tenfold in molecular weight had similar effects on the sedimentation of albumin. The results that have been noted are in accord with the view that chain polymers form three-dimensional networks which act as macromolecular sieves.},
