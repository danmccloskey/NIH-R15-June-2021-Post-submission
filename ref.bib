@article{gonzalez-islas_2009_nkcc1a,
  ids = {gonzalez-islas\_2009\_nkcc1},
  title = {{{NKCC1}} and {{AE3 Appear}} to {{Accumulate Chloride}} in {{Embryonic Motoneurons}}},
  author = {{Gonzalez-Islas}, Carlos and Chub, Nikolai and Wenner, Peter},
  year = {2009},
  month = feb,
  volume = {101},
  pages = {507--518},
  issn = {0022-3077, 1522-1598},
  doi = {10.1152/jn.90986.2008},
  abstract = {During early development, {$\gamma$}-aminobutyric acid (GABA) depolarizes and excites neurons, contrary to its typical function in the mature nervous system. As a result, developing networks are hyperexcitable and experience a spontaneous network activity that is important for several aspects of development. GABA is depolarizing because chloride is accumulated beyond its passive distribution in these developing cells. Identifying all of the transporters that accumulate chloride in immature neurons has been elusive and it is unknown whether chloride levels are different at synaptic and extrasynaptic locations. We have therefore assessed intracellular chloride levels specifically at synaptic locations in embryonic motoneurons by measuring the GABAergic reversal potential (EGABA) for GABAA miniature postsynaptic currents. When whole cell patch solutions contained 17\textendash 52 mM chloride, we found that synaptic EGABA was around -30 mV. Because of the low HCO3- permeability of the GABAA receptor, this value of EGABA corresponds to approximately 50 mM intracellular chloride. It is likely that synaptic chloride is maintained at levels higher than the patch solution by chloride accumulators. We show that the Na+-K+-2Cl- cotransporter, NKCC1, is clearly involved in the accumulation of chloride in motoneurons because blocking this transporter hyperpolarized EGABA and reduced nerve potentials evoked by local application of a GABAA agonist. However, chloride accumulation following NKCC1 block was still clearly present. We find physiological evidence of chloride accumulation that is dependent on HCO3- and sensitive to an anion exchanger blocker. These results suggest that the anion exchanger, AE3, is also likely to contribute to chloride accumulation in embryonic motoneurons.},
  copyright = {Copyright \textcopyright{} 2009 the American Physiological Society},
  eprint = {19036864},
  eprinttype = {pmid},
  file = {/Users/danmccloskey/Zotero/storage/2Q7Y6E33/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.pdf;/Users/danmccloskey/Zotero/storage/3J9Q5HZJ/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.pdf;/Users/danmccloskey/Zotero/storage/9ZMK5C7N/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.pdf;/Users/danmccloskey/Zotero/storage/K2I39BZJ/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.pdf;/Users/danmccloskey/Zotero/storage/LULBP7EC/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.pdf;/Users/danmccloskey/Zotero/storage/XNI28UBB/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.pdf;/Users/danmccloskey/Zotero/storage/GPAJIF2C/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.html;/Users/danmccloskey/Zotero/storage/I9SHBWCI/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.html;/Users/danmccloskey/Zotero/storage/MN9DR7C7/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.html;/Users/danmccloskey/Zotero/storage/PUHGSX3X/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.html;/Users/danmccloskey/Zotero/storage/QUHSIB7X/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.html;/Users/danmccloskey/Zotero/storage/U6KBVFUC/Gonzalez-Islas et al. - 2009 - NKCC1 and AE3 Appear to Accumulate Chloride in Emb.html},
