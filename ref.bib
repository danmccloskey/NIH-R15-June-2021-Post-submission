@article{peterson_2012_adult,
  ids = {peterson\_2012\_adulta},
  title = {Adult Naked Mole-Rat Brain Retains the {{NMDA}} Receptor Subunit {{GluN2D}} Associated with Hypoxia Tolerance in Neonatal Mammals},
