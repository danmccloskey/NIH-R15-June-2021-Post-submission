@article{zhang_2013_40millionyeara,
  ids = {zhang\_2013\_40millionyear},
  title = {A 40-Million-Year History of Atmospheric {{CO2}}},
  author = {Zhang, Yi Ge and Pagani, Mark and Liu, Zhonghui and Bohaty, Steven M. and DeConto, Robert},
  year = {2013},
  month = oct,
  volume = {371},
  pages = {20130096},
  issn = {1364-503X, 1471-2962},
  doi = {10.1098/rsta.2013.0096},
  abstract = {The alkenone\textendash pCO2 methodology has been used to reconstruct the partial pressure of ancient atmospheric carbon dioxide (pCO2) for the past 45 million years of Earth's history (Middle Eocene to Pleistocene epochs). The present long-term CO2 record is a composite of data from multiple ocean localities that express a wide range of oceanographic and algal growth conditions that potentially bias CO2 results. In this study, we present a pCO2 record spanning the past 40 million years from a single marine locality, Ocean Drilling Program Site 925 located in the western equatorial Atlantic Ocean. The trends and absolute values of our new CO2 record site are broadly consistent with previously published multi-site alkenone\textendash CO2 results. However, new pCO2 estimates for the Middle Miocene are notably higher than published records, with average pCO2 concentrations in the range of 400\textendash 500 ppm. Our results are generally consistent with recent pCO2 estimates based on boron isotope-pH data and stomatal index records, and suggest that CO2 levels were highest during a period of global warmth associated with the Middle Miocene Climatic Optimum (17\textendash 14 million years ago, Ma), followed by a decline in CO2 during the Middle Miocene Climate Transition (approx. 14 Ma). Several relationships remain contrary to expectations. For example, benthic foraminiferal {$\delta$}18O records suggest a period of deglaciation and/or high-latitude warming during the latest Oligocene (27\textendash 23 Ma) that, based on our results, occurred concurrently with a long-term decrease in CO2 levels. Additionally, a large positive {$\delta$}18O excursion near the Oligocene\textendash Miocene boundary (the Mi-1 event, approx. 23 Ma), assumed to represent a period of glacial advance and retreat on Antarctica, is difficult to explain by our CO2 record alone given what is known of Antarctic ice sheet history and the strong hysteresis of the East Antarctic Ice Sheet once it has grown to continental dimensions. We also demonstrate that in the Neogene with low CO2 levels, algal carbon concentrating mechanisms and spontaneous biocarbonate\textendash CO2 conversions are likely to play a more important role in algal carbon fixation, which provides a potential bias to the alkenone\textendash pCO2 method.},
  copyright = {\textcopyright{} 2013 The Author(s) Published by the Royal Society. All rights reserved.},
  eprint = {24043869},
  eprinttype = {pmid},
  file = {/Users/danmccloskey/Zotero/storage/BQTMYPWL/Zhang et al. - 2013 - A 40-million-year history of atmospheric CO2.pdf;/Users/danmccloskey/Zotero/storage/J9KN3YNB/Zhang et al. - 2013 - A 40-million-year history of atmospheric CO2.pdf;/Users/danmccloskey/Zotero/storage/VZFMHIB8/Zhang et al. - 2013 - A 40-million-year history of atmospheric CO2.pdf;/Users/danmccloskey/Zotero/storage/4DBX6T7P/20130096.html;/Users/danmccloskey/Zotero/storage/5CJ9TJSC/20130096.html;/Users/danmccloskey/Zotero/storage/PVUQRH33/20130096.html},
  journal = {Phil. Trans. R. Soc. A},
  keywords = {\#duplicate-citation-key},
  language = {en},
  number = {2001},
  pmid = {24043869}
}

@article{zhang_2019_fossorial,
  title = {Fossorial {{Damaraland}} Mole Rats Do Not Exhibit a Blunted Hypercapnic Ventilatory Response},
  author = {Zhang, Sarah Y. and Pamenter, Matthew E.},
  year = {2019},
  month = mar,
  volume = {15},
  pages = {20190006},
  doi = {10.1098/rsbl.2019.0006},
  abstract = {Damaraland mole rats (DMRs, Fukomys damarensis) are a eusocial fossorial species that spend the majority of their life in densely populated underground burrows, in which they likely experience intermittent periods of elevated CO2 (i.e. hypercapnia). The primary physiological response to hypercapnia in most mammals is to increase depth and rate of breathing (i.e. hyperpnoea), but this response is often blunted in species that inhabit hypercapnic environments. In their natural habitat, DMRs putatively experience a gaseous environment ranging from normocapnic (0.1\% CO2) to hypercapnic (6.0\% CO2) conditions (Roper et al. 2001 J. Zool.254, 101\textendash 107). As such, we hypothesized that DMRs would exhibit blunted hypercapnic ventilatory and metabolic responses, relative to those of non-fossorial rodent species. To test this hypothesis, we exposed awake, freely behaving DMRs to normoxic normocapnia (21\% O2, 0\% CO2, balance N2) or graded normoxic hypercapnia (21\% O2, 0, 2, 5, 7 and 10\% CO2, balance N2), and measured ventilation and metabolism using whole-body plethysmography and indirect calorimetry, respectively. We found that ventilation and metabolism were unchanged during prolonged normocapnia, whereas during graded hypercapnia, ventilation was elevated at 2\% CO2 and above. As a result, O2 extraction efficiency at the lungs decreased with increasing hyperpnoea. Conversely, metabolic rate did not increase until 10\% CO2, presumably due to the metabolic cost of hyperpnoea. Taken together, our results suggest that despite their fossorial lifestyle, DMRs do not exhibit adaptations in their ventilatory or metabolic responses to environmental hypercapnia.},
  file = {/Users/danmccloskey/Zotero/storage/92LJHJQS/Zhang and Pamenter - 2019 - Fossorial Damaraland mole rats do not exhibit a bl.pdf;/Users/danmccloskey/Zotero/storage/QUVZ7Q6G/Zhang and Pamenter - 2019 - Fossorial Damaraland mole rats do not exhibit a bl.pdf;/Users/danmccloskey/Zotero/storage/XIWRP2I3/Zhang and Pamenter - 2019 - Fossorial Damaraland mole rats do not exhibit a bl.pdf;/Users/danmccloskey/Zotero/storage/76DZ2ECQ/rsbl.2019.html;/Users/danmccloskey/Zotero/storage/7HNAZVD9/rsbl.2019.html;/Users/danmccloskey/Zotero/storage/NX8EQTJY/rsbl.2019.html},
  journal = {Biology Letters},
  keywords = {\#duplicate-citation-key},
  number = {3}
}

