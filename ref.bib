@article{ben-ari_2012_gaba,
  ids = {ben-ari\_2012\_gabaa,ben-ari\_2012\_gabac,ben-ari\_2014\_gabac},
  title = {The {{GABA Excitatory}}/{{Inhibitory Shift}} in {{Brain Maturation}} and {{Neurological Disorders}}},
  author = {{Ben-Ari}, Yehezkel and Khalilov, Ilgam and Kahle, Kristopher T. and Cherubini, Enrico},
  year = {2012},
  month = oct,
  volume = {18},
  pages = {467--486},
  issn = {1073-8584},
  doi = {10.1177/1073858412438697},
  abstract = {Ionic currents and the network-driven patterns they generate differ in immature and adult neurons: The developing brain is not a ``small adult brain.'' One of the most investigated examples is the developmentally regulated shift of actions of the transmitter GABA that inhibit adult neurons but excite immature ones because of an initially higher intracellular chloride concentration [Cl-]i, leading to depolarizing and often excitatory actions of GABA instead of hyperpolarizing and inhibitory actions. The levels of [Cl-]i are also highly labile, being readily altered transiently or persistently by enhanced episodes of activity in relation to synaptic plasticity or a variety of pathological conditions, including seizures and brain insults. Among the plethora of channels, transporters, and other devices involved in controlling [Cl-]i, two have emerged as playing a particularly important role: the chloride importer NKCC1 and the chloride exporter KCC2. Here, the authors stress the importance of determining how [Cl-]i is dynamically regulated and how this affects brain operation in health and disease. In a clinical perspective, agents that control [Cl-]i and reinstate inhibitory actions of GABA open novel therapeutic perspectives in many neurological disorders, including infantile epilepsies, autism spectrum disorders, and other developmental disorders.},
  file = {/Users/danmccloskey/Zotero/storage/2YDQ78JP/Ben-Ari et al. - 2012 - The GABA ExcitatoryInhibitory Shift in Brain Matu.pdf;/Users/danmccloskey/Zotero/storage/E8ZYZHFN/Ben-Ari et al. - 2012 - The GABA ExcitatoryInhibitory Shift in Brain Matu.pdf;/Users/danmccloskey/Zotero/storage/FB8JYJWQ/Ben-Ari et al. - 2012 - The GABA ExcitatoryInhibitory Shift in Brain Matu.pdf;/Users/danmccloskey/Zotero/storage/T2C7PY2T/Ben-Ari et al. - 2012 - The GABA ExcitatoryInhibitory Shift in Brain Matu.pdf;/Users/danmccloskey/Zotero/storage/UHS9HNQW/Ben-Ari et al. - 2012 - The GABA ExcitatoryInhibitory Shift in Brain Matu.pdf;/Users/danmccloskey/Zotero/storage/VJ8M3WXW/Ben-Ari et al. - 2012 - The GABA ExcitatoryInhibitory Shift in Brain Matu.pdf;/Users/danmccloskey/Zotero/storage/5YPUMVM8/Ben-Ari - 2014 - The GABA excitatoryinhibitory developmental seque.html;/Users/danmccloskey/Zotero/storage/E59MRABZ/Ben-Ari - 2014 - The GABA excitatoryinhibitory developmental seque.html;/Users/danmccloskey/Zotero/storage/HAZQDKUA/Ben-Ari - 2014 - The GABA excitatoryinhibitory developmental seque.html;/Users/danmccloskey/Zotero/storage/K5I3P9K6/Ben-Ari - 2014 - The GABA excitatoryinhibitory developmental seque.html;/Users/danmccloskey/Zotero/storage/NHANL5QF/Ben-Ari - 2014 - The GABA excitatoryinhibitory developmental seque.html;/Users/danmccloskey/Zotero/storage/SQYIKI64/Ben-Ari - 2014 - The GABA excitatoryinhibitory developmental seque.html;/Users/danmccloskey/Zotero/storage/ZIBBAG9L/Ben-Ari - 2014 - The GABA excitatoryinhibitory developmental seque.html},
