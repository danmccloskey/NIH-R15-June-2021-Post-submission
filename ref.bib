}

@article{connell_1989_clinicalb,
  ids = {connell\_1989\_clinicala,connell\_1989\_clinicalc},
  title = {Clinical and {{EEG}} Response to Anticonvulsants in Neonatal Seizures},
  author = {Connell, J. and Oozeer, R. and {de Vries}, L. and Dubowitz, L. M. and Dubowitz, V.},
