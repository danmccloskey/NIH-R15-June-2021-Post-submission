  title = {Characterization of Febrile Seizures and Febrile Seizure Susceptibility in Mouse Inbred Strains},
  author = {Van Gassen, K. L. I. and Hessel, E. V. S. and Ramakers, G. M. J. and Notenboom, R. G. E. and {Wolterink-Donselaar}, I. G. and Brakkee, J. H. and Godschalk, T. C. and Qiao, X. and Spruijt, B. M. and Van Nieuwenhuizen, O. and De Graan, P. N. E.},
  year = {2008},
