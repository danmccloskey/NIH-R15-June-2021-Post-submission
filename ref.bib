  pmid = {20842164}
}

@article{yahav_1991_huddling,
  ids = {yahav\_1991\_huddlinga,yahav\_1991\_huddlingc},
  title = {Huddling {{Behavior Facilitates Homeothermy}} in the {{Naked Mole Rat Heterocephalus}} Glaber},
  author = {Yahav, Shlomo and Buffenstein, Rochelle},
  year = {1991},
  volume = {64},
  pages = {871--884},
  issn = {0031-935X},
  abstract = {Heterocephalus glaber (the naked mole rat) is a highly social mammal, living in large underground colonies in equatorial Africa. Isolated naked mole rats exhibit unusual thermoregulation and cannot regulate body temperature (\$T\_\{b\}\$). Thermoregulatory parameters, namely Vo{$_2$}, evaporative water loss (EWL), and \$T\_\{b\}\$, were therefore examined when these animals were housed in groups ranging from two to eight animals. Oxygen consumption at low ambient temperatures (\$T\_\{a\},'s\$), irrespective of the experimental group size, increased with increasing \$T\_\{a\}\$ in a poikilothermic manner. Changes in Vo{$_2$} with increasing \$T\_\{a\}\$ switched to an endothermic pattern at \$T\_\{a\}'s\$ ranging from 25\textdegree C (for groups of eight) to 27\textdegree{} C (for pairs). Huddling behavior confers savings in both energy expenditure and evaporative water loss. These are most pronounced at high \$T\_\{a\} 's\$. For instance, at \$T\_\{a\}\$ = 32\textdegree{} C (within the "thermoneutral zone "), groups of eight animals use 78\% of the Vo{$_2$} and 69\% of EWL used by pairs. Body temperature of animals housed in pairs is directly proportional to \$T\_\{a\}\$ over the entire experimental \$T\_\{a\}\$ range (14\textdegree 37\textdegree{} C). Over the \$T\_\{a\}\$ range normally encountered in their warm equatorial burrows (29\textdegree{} -32\textdegree{} C), animals that were housed in groups of four or more tightly controlled \$T\_\{b\}\$ (within 1\textdegree{} C). Data presented here suggest that huddling behavior not only saves energy and water, so essential in an arid environment where food is sparsely distributed, but huddling also plays a very important thermoregulatory role in these otherwise nonendothermic mammals. Huddling ensures that, in their natural habitat, naked mole rats are able to control \$T\_\{b\}\$ and are indeed homeotherms.},
  file = {/Users/danmccloskey/Zotero/storage/79UPVQLA/Yahav and Buffenstein - 1991 - Huddling Behavior Facilitates Homeothermy in the N.pdf;/Users/danmccloskey/Zotero/storage/KVQZE72M/Yahav and Buffenstein - 1991 - Huddling Behavior Facilitates Homeothermy in the N.pdf;/Users/danmccloskey/Zotero/storage/QGFT5EWM/Yahav and Buffenstein - 1991 - Huddling Behavior Facilitates Homeothermy in the N.pdf;/Users/danmccloskey/Zotero/storage/S9UG5WSH/physzool.64.3.html;/Users/danmccloskey/Zotero/storage/SFW8G4RI/physzool.64.3.html;/Users/danmccloskey/Zotero/storage/XHTQVIGY/physzool.64.3.html},
  journal = {Physiological Zoology},
  keywords = {\#duplicate-citation-key},
  number = {3}
}

