@article{oriain_2000_morphological,
  title = {Morphological Castes in a Vertebrate},
  author = {O'Riain, M. J. and Jarvis, J. U. M. and Alexander, R. and Buffenstein, R. and Peeters, C.},
  year = {2000},
  month = nov,
  volume = {97},
  pages = {13194--13197},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.97.24.13194},
  abstract = {Morphological specialization for a specific role has, until now, been assumed to be restricted to social invertebrates. Herein we show that complete physical dimorphism has evolved between reproductives and helpers in the eusocial naked mole-rat. Dimorphism is a consequence of the lumbar vertebrae lengthening after the onset of reproduction in females. This is the only known example of morphological castes in a vertebrate and is distinct from continuous size variation between breeders and helpers in other species of cooperatively breeding vertebrates. The evolution of castes in a mammal and insects represents a striking example of convergent evolution for enhanced fecundity in societies characterized by high reproductive skew. Similarities in the selective environment between naked mole-rats and eusocial insect species highlight the selective conditions under which queen/worker castes are predicted to evolve in animal societies.},
  eprint = {11087866},
  eprinttype = {pmid},
  file = {/Users/danmccloskey/Zotero/storage/85LYHAZJ/O'Riain et al. - 2000 - Morphological castes in a vertebrate.pdf;/Users/danmccloskey/Zotero/storage/T3J7ZH36/13194.html},
  journal = {PNAS},
  language = {English},
  number = {24}
}

