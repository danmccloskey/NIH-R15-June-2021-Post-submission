@article{zions_2020_nest,
  title = {Nest {{Carbon Dioxide Masks GABA}}-{{Dependent Seizure Susceptibility}} in the {{Naked Mole}}-{{Rat}}},
  author = {Zions, Michael and Meehan, Edward F. and Kress, Michael E. and Thevalingam, Donald and Jenkins, Edmund C. and Kaila, Kai and Puskarjov, Martin and McCloskey, Dan P.},
  year = {2020},
  month = jun,
  volume = {30},
  pages = {2068-2077.e4},
  issn = {09609822},
  doi = {10.1016/j.cub.2020.03.071},
  abstract = {African naked mole-rats were likely the first mammals to evolve eusociality, and thus required adaptations to conserve energy and tolerate the low oxygen (O2) and high carbon dioxide (CO2) of a densely populated fossorial nest. As hypercapnia is known to suppress neuronal activity, we studied whether naked mole-rats might demonstrate energy savings in GABAergic inhibition. Using whole-colony behavioral monitoring of captive naked mole-rats, we found a durable nest, characterized by high CO2 levels, where all colony members spent the majority of their time. Analysis of the naked mole-rat genome revealed, uniquely among mammals, a histidine point variation in the neuronal potassium-chloride cotransporter 2 (KCC2). A histidine missense substitution mutation at this locus in the human ortholog of KCC2, found previously in patients with febrile seizures and epilepsy, has been demonstrated to diminish neuronal Cl\`A extrusion capacity, and thus impairs GABAergic inhibition. Seizures were observed, without pharmacological intervention, in adult naked mole-rats exposed to a simulated hyperthermic surface environment, causing systemic hypocapnic alkalosis. Consistent with the diminished function of KCC2, adult naked mole-rats demonstrate a reduced efficacy of inhibition that manifests as triggering of seizures at room temperature by the GABAA receptor (GABAAR) positive allosteric modulator diazepam. These seizures are blocked in the presence of nest-like levels of CO2 and likely to be mediated through GABAAR activity, based on in vitro recordings. Thus, altered GABAergic inhibition adds to a growing list of adaptations in the naked mole-rat and provides a plausible proximate mechanism for nesting behavior, where a return to the colony nest restores GABA-mediated inhibition.},
  file = {/Users/danmccloskey/Zotero/storage/7A3WIKW9/Zions et al. - 2020 - Nest Carbon Dioxide Masks GABA-Dependent Seizure S.pdf},
  journal = {Current Biology},
  language = {en},
  number = {11}
}

@article{zoremba_2007_brain,
  title = {Brain Metabolism and Extracellular Space Diffusion Parameters during and after Transient Global Hypoxia in the Rat Cortex},
  author = {Zoremba, Norbert and Homola, Ale{\v s} and Rossaint, Rolf and Sykov{\'a}, Eva},
  year = {2007},
  month = jan,
  volume = {203},
  pages = {34--41},
  issn = {0014-4886},
  doi = {10.1016/j.expneurol.2006.07.017},
  abstract = {Hypoxia results in both reversible and irreversible changes in the brain extracellular space (ECS). This study utilized microdialysis to monitor changes in the energy-related metabolites lactate, pyruvate, glucose and glutamate in the rat cortex before, during and after 30-min transient global hypoxia, induced in anesthetized rats by reducing inspired oxygen to 6\% O2 in nitrogen. Changes in metabolite levels were compared with ECS diffusion parameters calculated from diffusion curves of tetramethylammonium applied by iontophoresis. Significant increases in lactate concentration and the lactate/pyruvate ratio, as well as decreased glucose levels, were found in the cortex immediately after the induction of hypoxia. Following recovery to ventilation with air, extracellular lactate and glucose levels and the lactate/pyruvate ratio returned to control levels within 40, 20 and 30~min, respectively. Glutamate levels started to increase 20\textendash 30~min after the onset of hypoxia and returned to prehypoxic values within 30\textendash 40~min of reoxygenation. The ECS volume fraction {$\alpha$} decreased by about 5\% from 0.18{$\pm$}0.01 during the first 20\textendash 25~min of hypoxia; after 25~min {$\alpha$} dropped a further 22\% to 0.14{$\pm$}0.01. Within 10~min of reoxygenation, {$\alpha$} returned to control values, then increased to 0.20{$\pm$}0.01 and remained at this level until the end of the experiment. The observed 22\% decrease in {$\alpha$} markedly influences dialysate levels measured during hypoxia. In our study, the complete posthypoxic recovery of cortical metabolite levels and ECS diffusion properties suggests that metabolic enzymes and related cellular components (e.g., mitochondria) may tolerate prolonged hypoxic periods and recover to prehypoxic values.},
  file = {/Users/danmccloskey/Zotero/storage/7FDHLJZ2/S0014488606004341.html},
  journal = {Experimental Neurology},
  keywords = {Cortex,DC potential,Extracellular space,Glutamate,Hypoxia,Lactate,Microdialysis,Recovery},
  language = {en},
  number = {1}
}


