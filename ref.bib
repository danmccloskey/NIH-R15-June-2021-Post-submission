@article{kahle_2013_modulation,
  title = {Modulation of Neuronal Activity by Phosphorylation of the {{K}}-{{Cl}} Cotransporter {{KCC2}}},
  author = {Kahle, Kristopher T. and Deeb, Tarek Z. and Puskarjov, Martin and Silayeva, Liliya and Liang, Bo and Kaila, Kai and Moss, Stephen J.},
  year = {2013},
  month = dec,
  volume = {36},
  pages = {726--737},
  issn = {1878-108X},
  doi = {10.1016/j.tins.2013.08.006},
  abstract = {The K-Cl cotransporter KCC2 establishes the low intraneuronal Cl- levels required for the hyperpolarizing inhibitory postsynaptic potentials mediated by ionotropic {$\gamma$}-aminobutyric acid receptors (GABAARs) and glycine receptors (GlyRs). Decreased KCC2-mediated Cl- extrusion and impaired hyperpolarizing GABAAR- and/or GlyR-mediated currents have been implicated in epilepsy, neuropathic pain, and spasticity. Recent evidence suggests that the intrinsic ion transport rate, cell surface stability, and plasmalemmal trafficking of KCC2 are rapidly and reversibly modulated by the (de)phosphorylation of critical serine, threonine, and tyrosine residues in the C terminus of this protein. Alterations in KCC2 phosphorylation have been associated with impaired KCC2 function in several neurological diseases. Targeting KCC2 phosphorylation directly or indirectly via upstream regulatory kinases might be a novel strategy to modulate GABA- and/or glycinergic signaling for therapeutic benefit.},
  eprint = {24139641},
  eprinttype = {pmid},
  file = {/Users/danmccloskey/Zotero/storage/ENBS28BA/Kahle et al. - 2013 - Modulation of neuronal activity by phosphorylation.pdf},
  journal = {Trends Neurosci.},
  keywords = {Animals,Biological,GABA,Glycine,Humans,Models,Nervous System Diseases,Neurons,Phosphorylation,Receptors,Signal Transduction,Symporters},
  language = {English},
  number = {12},
  pmcid = {PMC4381966}
}

