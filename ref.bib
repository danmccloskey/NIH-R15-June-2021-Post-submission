@article{bergstrom_2013_automated,
  title = {Automated Identification of Multiple Seizure-Related and Interictal Epileptiform Event Types in the {{EEG}} of Mice},
  author = {Bergstrom, Rachel A. and Choi, Jee Hyun and Manduca, Armando and Shin, Hee-Sup and Worrell, Greg A. and Howe, Charles L.},
  year = {2013},
  month = mar,
  volume = {3},
  pages = {1483},
  issn = {2045-2322},
  doi = {10.1038/srep01483},
  abstract = {Visual scoring of murine EEG signals is time-consuming and subject to low inter-observer reproducibility. The Racine scale for behavioral seizure severity does not provide information about interictal or sub-clinical epileptiform activity. An automated algorithm for murine EEG analysis was developed using total signal variation and wavelet decomposition to identify spike, seizure, and other abnormal signal types in single-channel EEG collected from kainic acid-treated mice. The algorithm was validated on multi-channel EEG collected from {$\gamma$}-butyrolacetone-treated mice experiencing absence seizures. The algorithm identified epileptiform activity with high fidelity compared to visual scoring, correctly classifying spikes and seizures with 99\% accuracy and 91\% precision. The algorithm correctly identifed a spike-wave discharge focus in an absence-type seizure recorded by 36 cortical electrodes. The algorithm provides a reliable and automated method for quantification of multiple classes of epileptiform activity within the murine EEG and is tunable to a variety of event types and seizure categories.},
  copyright = {2013 Nature Publishing Group},
  file = {/Users/danmccloskey/Zotero/storage/RZIA7C5F/Bergstrom et al. - 2013 - Automated identification of multiple seizure-relat.pdf;/Users/danmccloskey/Zotero/storage/W5KNRERT/Bergstrom et al. - 2013 - Automated identification of multiple seizure-relat.pdf;/Users/danmccloskey/Zotero/storage/XIEMGVTP/Bergstrom et al. - 2013 - Automated identification of multiple seizure-relat.pdf;/Users/danmccloskey/Zotero/storage/2GE4DA7C/srep01483.html;/Users/danmccloskey/Zotero/storage/E3GIKAUQ/srep01483.html;/Users/danmccloskey/Zotero/storage/WS7WRZ97/srep01483.html},
