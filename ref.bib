@book{deeb_2013_disrupteda,
  title = {Disrupted {{Cl}} - Homeostasis Contributes to Reductions in the Inhibitory Efficacy of Diazepam during Hyperexcited States},
  author = {Deeb, Tarek and Nakamura, Yasuko and D Frost, Greg and Davies, Paul and J Moss, Stephen},
  year = {2013},
  month = apr,
  volume = {38},
  doi = {10.1111/ejn.12241},
  abstract = {The K(+) -Cl(-) cotransporter type 2 is the major Cl(-) extrusion mechanism in most adult neurons. This process in turn leads to Cl(-) influx upon activation of {$\gamma$}-aminobutyric acid type A (GABAA ) receptors and the canonical hyperpolarising inhibitory postsynaptic potential. Several neurological disorders are treated with drugs that target and enhance GABAA receptor signaling, including the commonly used benzodiazepine diazepam and the anesthetic propofol. Some of these disorders are also associated with deficits in GABAA signaling and become less sensitive to therapeutic drugs that target GABAA receptors. To date, it is unknown if alterations in the neuronal Cl(-) gradient affect the efficacies of diazepam and propofol. We therefore used the in vitro model of glutamate-induced hyperexcitability to test if alterations in the Cl(-) gradient affect the efficacy of GABAA modulators. We exclusively utilised the gramicidin perforated-patch-clamp configuration to preserve the endogenous Cl(-) gradient in rat neurons. Brief exposure to glutamate reduced the inhibitory efficacy of diazepam within 5 min, which was caused by the collapse of the Cl(-) gradient, and not due to reductions in GABAA receptor number. Unlike diazepam, propofol retained its efficacy by shunting the membrane conductance despite the glutamate-induced appearance of depolarising GABAA -mediated currents. Similarly, pharmacological inhibition of K(+) -Cl(-) cotransporter type 2 by furosemide disrupted Cl(-) homeostasis and reduced the efficacy of diazepam but not propofol. Collectively our results suggest that pathological hyperexcitable conditions could cause the rapid accumulation of intracellular Cl(-) and the appearance of depolarising GABAA -mediated currents that would decrease the efficacy of diazepam.},
  file = {/Users/danmccloskey/Zotero/storage/27S5YLBN/Deeb et al. - 2013 - Disrupted Cl − homeostasis contributes to reductio.pdf},
  keywords = {\#duplicate-citation-key}
}

