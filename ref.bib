@article{roper_2001_environmental,
  ids = {roper\_2001\_environmental},
  title = {Environmental Conditions in Burrows of Two Species of {{African}} Mole-Rat, {{Georhychus}} Capensis and {{Cryptomys}} Damarensis},
  author = {Roper, T. J. and Bennett, N. C. and Conradt, L. and Molteno, A. J.},
  year = {2001},
  volume = {254},
  pages = {101--107},
  issn = {1469-7998},
  doi = {10.1017/S0952836901000590},
  abstract = {Temperature and respiratory gas (CO2 and O2) concentrations were measured in the foraging tunnels of burrows naturally inhabited by two species of southern African mole-rats, the Cape mole-rat Georhychus capensis and the Damaraland mole-rat Cryptomys damarensis. Both species are completely fossorial and inhabit closed burrow systems. Tunnels of G. capensis burrows had a mean diameter of 8.7 cm and a depth, measured to the roof of the tunnel, of 6.2 cm; those of C. damarensis had a mean diameter of 6.5 cm and depth of 40 cm. In both species, the mean concentration of CO2 was higher, and mean concentration of O2 lower, in burrows than in the surrounding soil or in ambient air. Mean and minimum values of O2 were 20.4\% and 19.8\%, respectively, in G. capensis and 20.4\% and 19.9\% in C. damarensis; mean and maximum values of CO2 were 0.4\% and 1.2\% in G. capensis and 0.4\% and 6.0\% in C. damarensis. Temperature varied between 18.5 and 24.2 \textdegree C in burrows of G. capensis by comparison with an ambient range of 16.9 to 26.8 \textdegree C; and from 19.6 to 29.3 \textdegree C in burrows of C. damarensis by comparison with an ambient range of 8.6 to 30.8 \textdegree C. Thus a burrowing habit seems to offer both species protection from extremes of temperature without entailing the cost of a grossly abnormal respiratory environment. From a review of the relevant literature, we conclude that average concentrations of CO2 and O2 in mammalian burrows often do not differ greatly from ambient values. However, more work is needed to determine the respiratory gas concentrations in the immediate vicinity of active, burrowing animals.},
  file = {/Users/danmccloskey/Zotero/storage/QHZTUUF2/Roper et al. - 2001 - Environmental conditions in burrows of two species.pdf;/Users/danmccloskey/Zotero/storage/USJCSWBH/Roper et al. - 2001 - Environmental conditions in burrows of two species.pdf;/Users/danmccloskey/Zotero/storage/RU5FN35J/S0952836901000590.html;/Users/danmccloskey/Zotero/storage/ZV6HVI36/S0952836901000590.html},
  journal = {Journal of Zoology},
  keywords = {\#duplicate-citation-key,burrows,Cryptomys damarensis,environmental conditions,Georhychus capensis,mole-rat},
  language = {en},
  number = {1}
}

