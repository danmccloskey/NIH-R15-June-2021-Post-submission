@article{payne_1997_functional,
  title = {Functional Characterization of the Neuronal-Specific {{K}}-{{Cl}} Cotransporter: Implications for [{{K}}{\textsuperscript{+}}]{\textsubscript{o}}regulation},
  shorttitle = {Functional Characterization of the Neuronal-Specific {{K}}-{{Cl}} Cotransporter},
  author = {Payne, John A.},
  year = {1997},
  month = nov,
  volume = {273},
  pages = {C1516-C1525},
  issn = {0363-6143, 1522-1563},
  abstract = {The neuronal K-Cl cotransporter isoform (KCC2) was functionally expressed in human embryonic kidney (HEK-293) cell lines. Two stably transfected HEK-293 cell lines were prepared: one expressing an epitope-tagged KCC2 (KCC2\textendash 22T) and another expressing the unaltered KCC2 (KCC2\textendash 9). The KCC2\textendash 22T cells produced a glycoprotein of {$\sim$}150 kDa that was absent from HEK-293 control cells. The 86Rb influx in both cell lines was significantly greater than untransfected control HEK-293 cells. The KCC2\textendash 9 cells displayed a constitutively active86Rb influx that could be increased further by 1 mMN-ethylmaleimide (NEM) but not by cell swelling. Both furosemide [inhibition constant (K i) {$\sim$}25 {$\mu$}M] and bumetanide (Ki{$\sim$}55 {$\mu$}M) inhibited the NEM-stimulated86Rb influx in the KCC2\textendash 9 cells. This diuretic-sensitive86Rb influx in the KCC2\textendash 9 cells, operationally defined as KCC2 mediated, required external Cl-but not external Na+ and exhibited a high apparent affinity for external Rb+(K+) [Michaelis constant (K m) = 5.2 {$\pm$} 0.9 (SE) mM; n = 5] but a low apparent affinity for external Cl-(K m {$>$}50 mM). On the basis of thermodynamic considerations as well as the unique kinetic properties of the KCC2 isoform, it is hypothesized that KCC2 may serve a dual function in neurons: 1) the maintenance of low intracellular Cl- concentration so as to allow Cl- influx via ligand-gated Cl- channels and 2) the buffering of external K+ concentration ([K+]o) in the brain.},
  copyright = {Copyright \textcopyright{} 1997 the American Physiological Society},
  file = {/Users/danmccloskey/Zotero/storage/7S8WHRWD/Payne - 1997 - Functional characterization of the neuronal-specif.pdf;/Users/danmccloskey/Zotero/storage/7SZSX9FV/Payne - 1997 - Functional characterization of the neuronal-specif.pdf;/Users/danmccloskey/Zotero/storage/973S48AV/Payne - 1997 - Functional characterization of the neuronal-specif.pdf;/Users/danmccloskey/Zotero/storage/JB7B4DKM/Payne - 1997 - Functional characterization of the neuronal-specif.pdf;/Users/danmccloskey/Zotero/storage/KMBFAHHX/Payne - 1997 - Functional characterization of the neuronal-specif.pdf;/Users/danmccloskey/Zotero/storage/BDI47QVR/Payne - 1997 - Functional characterization of the neuronal-specif.html;/Users/danmccloskey/Zotero/storage/HHTDIQZM/Payne - 1997 - Functional characterization of the neuronal-specif.html;/Users/danmccloskey/Zotero/storage/M7SZPA28/Payne - 1997 - Functional characterization of the neuronal-specif.html;/Users/danmccloskey/Zotero/storage/PA8Q298R/Payne - 1997 - Functional characterization of the neuronal-specif.html;/Users/danmccloskey/Zotero/storage/ZI5WEB2X/Payne - 1997 - Functional characterization of the neuronal-specif.html},
