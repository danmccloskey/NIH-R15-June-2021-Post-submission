@article{swift-gallant_2015_removala,
  ids = {swift-gallant\_2015\_removal},
  title = {Removal of Reproductive Suppression Reveals Latent Sex Differences in Brain Steroid Hormone Receptors in Naked Mole-Rats, {{Heterocephalus}} Glaber},
  author = {{Swift-Gallant}, Ashlyn and Mo, Kaiguo and Peragine, Deane E. and Monks, D. Ashley and Holmes, Melissa M.},
  year = {2015},
  month = dec,
  volume = {6},
  issn = {2042-6410},
  doi = {10.1186/s13293-015-0050-x},
  abstract = {Background
Naked mole-rats are eusocial mammals, living in large colonies with a single breeding female and 1\textendash 3 breeding males. Breeders are socially dominant, and only the breeders exhibit traditional sex differences in circulating gonadal steroid hormones and reproductive behaviors. Non-reproductive subordinates also fail to show sex differences in overall body size, external genital morphology, and non-reproductive behaviors. However, subordinates can transition to breeding status if removed from their colony and housed with an opposite-sex conspecific, suggesting the presence of latent sex differences. Here, we assessed the expression of steroid hormone receptor and aromatase messenger RNA (mRNA) in the brains of males and females as they transitioned in social and reproductive status.

Methods
We compared in-colony subordinates to opposite-sex subordinate pairs that were removed from their colony for either 1~day, 1~week, 1~month, or until they became breeders (i.e., produced a litter). Diencephalic tissue was collected and mRNA of androgen receptor (Ar), estrogen receptor alpha (Esr1), progesterone receptor (Pgr), and aromatase (Cyp19a1) was measured using qPCR. Testosterone, 17{$\beta$}-estradiol, and progesterone from serum were also measured.

Results
As early as 1~week post-removal, males exhibited increased diencephalic Ar mRNA and circulating testosterone, whereas females had increased Cyp19a1 mRNA in the diencephalon. At 1~month post-removal, females exhibited increased 17{$\beta$}-estradiol and progesterone. The largest changes in steroid hormone receptors were observed in breeders. Breeding females had a threefold increase in Cyp19a1 and fivefold increases in Esr1 and Pgr, whereas breeding males had reduced Pgr and increased Ar.

Conclusions
These data demonstrate~that sex differences in circulating gonadal steroids and hypothalamic gene expression emerge weeks to months after subordinate animals are removed from reproductive suppression in their home colony.},
  eprint = {26693002},
  eprinttype = {pmid},
  file = {/Users/danmccloskey/Zotero/storage/753XFJ3D/Swift-Gallant et al. - 2015 - Removal of reproductive suppression reveals latent.pdf;/Users/danmccloskey/Zotero/storage/P2CWDGM6/Swift-Gallant et al. - 2015 - Removal of reproductive suppression reveals latent.pdf;/Users/danmccloskey/Zotero/storage/WDDJLKA8/Swift-Gallant et al. - 2015 - Removal of reproductive suppression reveals latent.pdf},
  journal = {Biol Sex Differ},
  keywords = {\#duplicate-citation-key},
  pmcid = {PMC4676092},
  pmid = {26693002}
}

